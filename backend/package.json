{
  "name": "backend",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "start": "node --dns-result-order=ipv4first dist/app.js",
    "dev": "npx cross-env UV_TCP_IPV6ONLY=0 NODE_OPTIONS='--dns-result-order=ipv4first' ts-node --project tsconfig.server.json src/app.ts",
    "devlocal": "ts-node --project tsconfig.server.json src/app.ts",
    "build": "tsc --project tsconfig.build.json",
    "build:prod": "tsc --project tsconfig.build.json",
    "build:force": "tsc --project tsconfig.build.json --noEmitOnError false",
    "build:full": "tsc --project tsconfig.server.json",
    "worker": "node src/worker.js",
    "test": "jest",
    "test:unit": "jest --config jest.unit.config.js",
    "test:integration": "jest --config jest.integration.config.js",
    "test:e2e": "jest --config jest.e2e.config.js",
    "test:unit:coverage": "jest --config jest.unit.config.js --coverage",
    "test:integration:coverage": "jest --config jest.integration.config.js --coverage",
    "test:e2e:coverage": "jest --config jest.e2e.config.js --coverage",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:allure": "jest --config jest-allure.config.js",
    "allure:generate": "allure generate allure-results --clean -o allure-report",
    "allure:open": "allure open allure-report",
    "test:monitor": "npm run test:coverage && node scripts/test-monitoring.js",
    "test:flaky": "node scripts/flaky-tests-detector.js",
    "db:migrate": "prisma migrate deploy",
    "db:migrate:dev": "prisma migrate dev",
    "db:seed": "ts-node src/seeds/seed.ts",
    "db:seed:test": "ts-node src/seeds/test/seed.ts",
    "db:reset": "prisma migrate reset --force",
    "lint": "eslint --ext .ts,.tsx src/",
    "lint:fix": "eslint --ext .ts,.tsx src/ --fix",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.50.4",
    "axios": "^1.10.0",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "express-rate-limit": "^7.5.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.16.0",
    "pino-http": "^10.5.0",
    "pino-pretty": "^11.0.0",
    "winston": "^3.11.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.9.0",
    "@prisma/client": "^6.11.1",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jest": "^29.5.0",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/morgan": "^1.9.10",
    "@types/multer": "^1.4.0",
    "@types/node": "^22.14.0",
    "@types/pg": "^8.15.4",
    "@types/supertest": "^6.0.0",
    "allure-commandline": "^2.34.1",
    "bcryptjs": "^3.0.2",
    "ioredis": "^5.6.1",
    "jest": "^29.7.0",
    "jest-allure": "^0.1.3",
    "nock": "^13.4.0",
    "nodemon": "^3.1.10",
    "supertest": "^6.3.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "~5.7.2"
  }
}
